{
  "title": "LeviBot Overview",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Requests / s (by endpoint)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(levibot_http_requests_total[1m])) by (endpoint)",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (%)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "job:http_request_errors:rate5m",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" } }
    },
    {
      "type": "timeseries",
      "title": "p95 Latency (s)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "job:endpoint:latency_p95_seconds",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "type": "timeseries",
      "title": "Events RPS (POSITION_CLOSED)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "job:events_position_closed_rps",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    }
  ],
  "templating": { "list": [] },
  "time": { "from": "now-15m", "to": "now" },
  "refresh": "5s"
}



